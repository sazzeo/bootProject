{{>layout/header}}
    <h1>게시글 등록</h1>
    <div class="col-md-12">
        <div class="col-md-4">
            <form>
                <div class="form-group">
                    <label for="id">제목</label>
                    <input type="text" class="form-control" id="id" placeholder="글번호" readonly="readonly" value="{{post.id}}">
                </div>
                <div class="form-group">
                    <label for="title">제목</label>
                    <input type="text" class="form-control" id="title" placeholder="제목을입력하세요"  value="{{post.title}}">
                </div>
                <div class="form-group">
                    <label for="author">작성자</label>
                    <input type="text" class="form-control" id="author" placeholder="작성자를 입력하세요" readonly="readonly"  value="{{post.author}}">
                </div>
                <div class="form-group">
                    <label for="content">내용</label>
                    <input type="text" class="form-control" id="content" placeholder="내용을 입력하세요"  value="{{post.content}}">
                </div>
            </form>
            <a href="/" role="botton" class="btn btn-secondary">취소</a>
            <button type="button"  class="btn btn-primary" id="btn-update">등록</button>
            <button type="button"  class="btn btn-danger" id="btn-delete">삭제</button>

        </div>
    </div>


{{>layout/footer}}